def arithmetic_operation(a, b, c):
    if(c == '+'):
        print(a + b)
    elif(c == '-'):
        print(a-b)
    elif(c == '*'):
        print(a * b)
    elif(c == '/'):
        print(a / b)
    else:
        print("Invalid operation")

num1 = int(input ("Enter the first number: "))
num2 = int(input ("Enter the second number: "))
num3 = input ("Enter an opertaion(+, -, *, /): ") 
obj = arithmetic_operation(num1, num2, num3)
